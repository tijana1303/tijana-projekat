package org.projekat1.model;


public class Ticket {
	private String ticketCode;
    private String type;
    private double price;
    private String section;
    private Event event;
    

    public Ticket() {
    }

    public Ticket(String ticketCode, String type, double price, String section, Event event) {
        this.type = type;
        this.price = price;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public double getPrice() {
        return price;
    }

    public void setPrice(double price) {
        this.price = price;
    }
}

